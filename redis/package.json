{
  "name": "tab-redis",
  "version": "0.1.0",
  "description": "Redis service for Tab for a Cause.",
  "private": true,
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.8.0",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.4.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^27.4.0",
    "prettier": "^1.18.2",
    "serverless": "^1.65.0"
  },
  "scripts": {
    "build": "rm -rf ./build && cross-env NODE_ENV=production babel --out-dir='build' ./src/handler.js",
    "deploy": "echo \"Be sure to set AWS_ACCOUNT_ID=*** and set SLS_STAGE to dev or prod.\" && yarn run build && serverless deploy -v",
    "remove": "echo \"Be sure to set AWS_ACCOUNT_ID=*** and set SLS_STAGE to dev or prod.\" && serverless remove",
    "format:fix": "prettier \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --write",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gladly-team/tab.git"
  },
  "author": "Gladly Team",
  "bugs": {
    "url": "https://github.com/gladly-team/tab/issues"
  },
  "homepage": "https://github.com/gladly-team/tab#readme",
  "dependencies": {
    "bluebird": "^3.7.1",
    "redis": "^3.1.1"
  }
}
